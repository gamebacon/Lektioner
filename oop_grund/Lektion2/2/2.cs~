using System.Collections.Generic;
using System;

public class Övning2 {


	public static void Main(string[] args) {

		new Övning2();

	}





	public Övning2() {

		int[] nums = new int[20];

		populate(nums);

		Array.Sort(nums);

		for(int i = 0; i < 3; i++)
			Console.WriteLine(nums[i]);



	}



	public void populate(int[] list) {

		Random random = new Random();

		for(int i = 0; i < 20; i++) {
			int num = random.Next(100);

			list[i] = num;
		}

	}


}
